public class Vetor1 {

	public static void main(String args[]){
		int num[] = new int[8];
		
		for(int n = 0; n < 8; n++){
			num[n] = Integer.parseInt(JOptionPane.showInputDialog("Informe o número para a posição " + i + " do vetor."));
		}
		
		for(int i = 0; i < 8; i++){

		}
		
		for(int a : num){
			System.out.println(a);
		}
		
		System.out.println("--------");
		
