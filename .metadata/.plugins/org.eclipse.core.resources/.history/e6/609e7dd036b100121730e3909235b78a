/*
 * 01. Preencher um vetor com 100 elementos inteiros fornecidos pelo usuário.
 * 02. Obter e imprimir o número de ocorrências de cada valor no vetor gerado.
 * */

import java.util.Arrays;

import javax.swing.*;
public class Vetor1 {

	public static void main(String args[]){
		int num[] = new int[8];
		
		for(int n = 0; n < 8; n++){
			num[n] = Integer.parseInt(JOptionPane.showInputDialog("Informe o número para a posição x do vetor."));
		}
		
		for(int i = 0; i < 8; i++){
			for(int j = 0; j < 8; j++)
